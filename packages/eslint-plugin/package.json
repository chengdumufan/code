{
  "name": "@magicspace/eslint-plugin",
  "version": "0.1.0",
  "description": "Custom ESLint rules for MagicSpace.",
  "author": "Chengdu Mufan Technology Co., Ltd.",
  "license": "MIT",
  "keywords": [
    "eslint"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "bld/index.js",
  "scripts": {
    "lint": "eslint src/tests/import-groups-test.ts --rulesdir bld/rules --ext .ts",
    "test:lint": "yarn build && yarn lint",
    "bare-test": "mocha ./bld/tests/*.js",
    "build": "rimraf bld && tsc --project src/tsconfig.json",
    "test": "yarn build && yarn bare-test"
  },
  "files": [
    "src",
    "!src/tests",
    "bld",
    "!bld/tests"
  ],
  "devDependencies": {
    "@types/eslint": "^6.1.1",
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.139",
    "@types/mocha": "^5.2.7",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "source-map-support": "^0.5.12",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/experimental-utils": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "eslint-plugin-import": "^2.18.2",
    "loadsh": "^0.0.4",
    "module-lens": "^0.1.3",
    "tslang": "^0.1.17"
  }
}
