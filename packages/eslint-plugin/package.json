{
  "name": "@mufan/eslint-plugin",
  "version": "0.1.38",
  "publishConfig": {
    "access": "public"
  },
  "description": "Custom ESLint rules for Mufan Code.",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "repository": "https://github.com/makeflow/mufan-code.git",
  "license": "MIT",
  "author": "Chengdu Mufan Technology Co., Ltd.",
  "main": "bld/index.js",
  "files": [
    "src",
    "!src/tests",
    "bld",
    "!bld/tests"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "bare-test": "mocha ./bld/tests/*.js -t 5000",
    "build": "rimraf bld && tsc --project src/tsconfig.json",
    "test": "yarn build && yarn bare-test"
  },
  "devDependencies": {
    "@types/diff": "^4.0.2",
    "mocha": "^8.0.1",
    "source-map-support": "^0.5.19"
  },
  "peerDependencies": {
    "eslint": "^6.6.0 || ^7",
    "typescript": "^3.5.3 || ^4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/experimental-utils": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "diff": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lodash": "^4.17.21",
    "module-lens": "^0.1.6",
    "tslang": "^0.1.23",
    "tsutils": "^3.20.0"
  }
}
