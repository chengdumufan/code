{
  "name": "@mufan/eslint-plugin",
  "version": "0.1.79",
  "publishConfig": {
    "access": "public"
  },
  "description": "Custom ESLint rules for Mufan Code.",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "repository": "https://github.com/makeflow/mufan-code.git",
  "license": "MIT",
  "author": "Chengdu Mufan Technology Co., Ltd.",
  "main": "bld/index.js",
  "files": [
    "src",
    "!src/tests",
    "bld",
    "!bld/tests"
  ],
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "bare-test": "mocha ./bld/tests/*.js -t 5000",
    "build-tests": "cd test/reference-missing-proof && yarn",
    "build": "rimraf bld && tsc --project src/tsconfig.json && yarn build-tests",
    "test": "yarn build && yarn bare-test"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@typescript-eslint/utils": "^5.59.5",
    "diff": "^5.1.0",
    "enhanced-resolve": "^5.14.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "module-lens": "^0.1.6",
    "tslang": "^0.1.27",
    "tsutils": "^3.21.0"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "optionalDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.21"
  }
}
