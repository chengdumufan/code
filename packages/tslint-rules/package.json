{
  "name": "@magicspace/tslint-rules",
  "version": "0.1.43",
  "description": "Custom TSLint rules for MagicSpace.",
  "repository": "https://github.com/makeflow/magicspace.git",
  "author": "Chengdu Mufan Technology Co., Ltd.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bare-test": "node ./bld/test/runner.js",
    "build": "rimraf bld && tsc --project src/rules && tsc --project src/test",
    "lint": "tslint --project src/rules && tslint --project src/test",
    "test": "yarn build && yarn bare-test && yarn lint"
  },
  "files": [
    "src/rules/**/*.ts",
    "bld/rules"
  ],
  "devDependencies": {
    "glob": "^7.1.4",
    "source-map-support": "^0.5.12"
  },
  "peerDependencies": {
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.14",
    "module-lens": "^0.1.3",
    "tslang": "^0.1.15",
    "tslib": "^1.10.0",
    "tsutils": "^3.14.0"
  }
}
